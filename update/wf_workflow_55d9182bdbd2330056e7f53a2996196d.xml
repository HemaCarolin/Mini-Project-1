        <value>// Set the variable 'answer' to a comma-separated list of user or group sys_ids that you want the email sent to.&#13;
var d=new GlideRecord('x_354366_mini_proj_blood_donars');&#13;
	d.addQuery('age_donar','&gt;',17);&#13;
	d.addQuery('donar_area','=',current.req_area).addOrCondition('donar_state','=',current.req_state);&#13;
if(current.blood_group.blood_group=='B+')&#13;
	{&#13;
		 d.addQuery('blood_group.blood_group','=','B+').addOrCondition('blood_group.blood_group','=','B-').addOrCondition('blood_group.blood_group','=','O+').addOrCondition('blood_group.blood_group','=','O-');&#13;
	d.query();&#13;
	}&#13;
while(d.next())&#13;
	{&#13;
		&#13;
		answer = answer+ d.getDisplayValue('email_donar');&#13;
		if(d.hasNext())&#13;
			{&#13;
				answer=answer+',';&#13;
			}&#13;
	}</value>
